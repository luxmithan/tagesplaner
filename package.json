{
  "name": "tagesplaner",
  "version": "1.0.0",
  "description": "Tagesplaner application to set goals",
  "author": "Luxmithan Sukumar",
  "main": "server.js",
  "scripts": {
    "install-server": "npm install",
    "install-client": "cd client && npm install",
    "install-all": "npm run install-client && npm install",
    "build-client": "cd client && npm run build",
    "start": "node server",
    "server": "nodemon server",
    "client": "cd client && npm run serve",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
